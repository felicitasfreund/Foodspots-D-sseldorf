            window.OptanonWrapper = function() {
                let allowedList = [
                    "AP-VID",
                    "HIA",
                    "auth_token",
                    "cur",
                    "isMobile",
                    "locale_code",
                    "platform",
                    "visitor_id",
                    "visitor_os",
                    "customer_id",
                    "TFE_DATE_FROM",
                    "TFE_DATE_TO",
                    "par_sess",
                    "dismissed_broadcast",
                    "session_message",
                    "force_logout",
                    "locale_autoredirect_optout",
                    "locale_autoredirect_origin",
                    "locale_autoredirect_deviceLocale",
                    "is_office_ip",
                    "disable_one_tap",
                    "SESSDATA",
                    "shopping_cart_id",
                    "ticketing_back_url",
                    "viasat_interaction_id",
                    "OptanonConsent",
                    "OptanonAlertBoxClosed",
                    "__utmzz",
                    "gyg_theme",
                    "gyg_theme_follow_system",
                    "debug_gettext_keys",
                    "TFE_booked_activity",
                    "geoip_country",
                    "geoip_city",
                    "geoip_region",
                    "is_synthetic",
                    "shoppingCartExpiration",
                    "csrfToken",
                    "tfeAppletName",
                    "tfeEndpointsRedirect",
                    "persist_date_date_from",
                    "persist_date_date_to",
                    "passesBundlesQualificationChecks"
                ];

                otDeleteCookie(allowedList);
                function otDeleteCookie(allowedList) {
                  let otDomainGrps = JSON.parse(JSON.stringify(Optanon.GetDomainData().Groups));
                  let otDeletedGrpIds = otGetInactiveId(OnetrustActiveGroups);
                  if (otDeletedGrpIds.length !== 0 && otDomainGrps.length !== 0) {
                    let cookiesArray = document.cookie.split(";");
                    for (let i = 0; i < otDomainGrps.length; i++) {
                      if (otDomainGrps[i].CustomGroupId !== '' && otDeletedGrpIds.includes(otDomainGrps[i].CustomGroupId)) {
                        for (let j = 0; j < otDomainGrps[i].Cookies.length; j++) {
                          let name = otDomainGrps[i].Cookies[j].Name;
                          // remove trailing x and find the matching cookie name
                          // _ga_xxxxxxx or ab.storage.sessionId.xxx or _dc_gtm_UA-xxxxxxxx
                          if (name.endsWith('xxx')) {
                            let length = name.length
                            for (let m = length - 1; m >= 0; m--) {
                                if (name.endsWith('x')) {
                                    name = name.substring(0, m);
                                }
                            }
                            for(let i = 0; i < cookiesArray.length; i++)
                            {
                                let cookie = cookiesArray[i].trim();
                                let cookieName = cookie.split("=")[0];
                                if (cookieName.startsWith(name)) {
                                    eraseCookie(cookieName, allowedList);
                                }
                            }
                          } else {
                            eraseCookie(name, allowedList);
                          }
                        }
                      }
                    }
                  }
                }
                function otGetInactiveId(otActiveGrp) {
                    customIniId = ',C0001,C0002,C0004,'.split(",");
                    customIniId = customIniId.filter(Boolean);
                    otActiveGrp = otActiveGrp.split(",");
                    otActiveGrp = otActiveGrp.filter(Boolean);
                    let result = [];
                    for (let i = 0; i < customIniId.length; i++) {
                      if (otActiveGrp.indexOf(customIniId[i]) <= -1) {
                        result.push(customIniId[i]);
                      }
                    }
                    return result;
                }
                function eraseCookie(name, allowedList) {
                  if (!allowedList.includes(name)) {
                      domainName = window.location.hostname;
                      parentDomainName = '.' + getParentDomain(domainName);
                      document.cookie = name + '=; Max-Age=-99999999; Path=/;Domain=' + parentDomainName;
                  }
                }
                // TLDs (top level domain) can contain special as well as latin characters.
                // A TLD's maximum length is 63 characters, although most are around 2–3.
                function getParentDomain(hostname) {
                    const MAX_TLD_LENGTH = 3;
                    function isNotTLD(_) { return _.length > MAX_TLD_LENGTH; }
                    hostname = hostname.split('.');
                    hostname = hostname.slice(Math.max(0, hostname.findLastIndex(isNotTLD)));
                    hostname = hostname.join('.');
                    return hostname;
                }

                function handleCloseBanner(e) {
                  if (e._OneTrustIntercepted) {
                    return;
                  }
                  e.stopPropagation();
                  const targetElement = e.target;

                  const banner = document.getElementById("onetrust-banner-sdk");
                  if (banner) {
                    banner.style.transition = "visibility 0s ease 400ms, opacity 400ms linear 0s";
                    banner.style.visibility = "hidden";
                    banner.style.opacity = 0;
                  }

                  setTimeout(() => {
                    const event = new Event("click", { bubbles: true, cancelable: true });
                    event._OneTrustIntercepted = true;
                    targetElement.dispatchEvent(event);
                  }, 0);
                }

                if (true) {
                  const oneTrustAcceptBtn = document.getElementById("onetrust-accept-btn-handler");
                  const oneTrustRejectBtn = document.getElementById("onetrust-reject-all-handler");
                  if (oneTrustAcceptBtn) {
                    oneTrustAcceptBtn.addEventListener("click", handleCloseBanner, true);
                  }
                  if (oneTrustRejectBtn) {
                    oneTrustRejectBtn.addEventListener("click", handleCloseBanner, true);
                  }
                }
            }
         </script>
    
    
